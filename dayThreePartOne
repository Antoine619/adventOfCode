with open("advent/input.txt") as f:
    lines = f.read().splitlines()

total = 0

for line in lines:
    l1, l2 = line[:len(line) // 2], line[len(line) // 2:]
    common = set(l1).intersection(l2)
    letter = common.pop()

    if letter.isupper():
        total += ord(letter) - ord("A") + 27
    else:
        total += ord(letter) - ord("a") + 1
        
print(total)
