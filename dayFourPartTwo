values = open('advent/input.txt')

count = 0

for line in values:
    firstGroup, secondGroup = line.split(',')

    firstNum, secondNum = map(int, firstGroup.split('-'))
    thirdNum, fourthNum = map(int, secondGroup.split('-'))

    if (secondNum < thirdNum) or (fourthNum < firstNum):
        count += 1

print(count)
